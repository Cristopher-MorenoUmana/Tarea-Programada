<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="480.0" prefWidth="640.0" spacing="20.0" stylesheets="@../../../CSS/Style.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.una.tarea_programada.MainMenuController">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Registro de torneos:">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Text>
      <Button fx:id="tournamentRegisterBtn" onAction="#switchToTournamentRegister" prefWidth="170.0" text="Registrar torneo">
         <VBox.margin>
            <Insets />
         </VBox.margin></Button>
      <Button fx:id="sportRegisterBtn" mnemonicParsing="false" onAction="#switchToSportRegister" prefWidth="170.0" text="Registrar deporte" />
      <Button fx:id="teamRegisterBtn" mnemonicParsing="false" onAction="#switchToTeamRegister" prefWidth="170.0" text="Registrar equipos" />
      <Button fx:id="clashBtn" mnemonicParsing="false" onAction="#switchToClashMenu" prefWidth="170.0" text="Llaves de enfrentamientos" />
      <Button fx:id="playBtn" mnemonicParsing="false" onAction="#switchToMatchMenu" prefWidth="170.0" text="Jugar los partidos" />
      <Button fx:id="participationBtn" mnemonicParsing="false" onAction="#switchToTeamParticipationMenu" prefWidth="170.0" text="Historial de equipo" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
